          <nav>
            <a href="/home">Home</a>
            <a href="/home/index/gallery">Gallery</a>
            <a href="/branduse/new/brands ">Brands</a>
          </nav>
<hr>
          <div class="header">
              <h1>Services We Offer</h1>
          </div>    
        
          <div class="list">
              <% for(let i = 0; i < Services.length; i++){ %>
                <p><a href="/home/services/<%=Services[i].id; %>"> <%=Services[i].typeOfService; %>  $<%= Services[i].price %> </a></p>
              <% } %>
            </div>
      </div>
    </div>


</body>

</html>